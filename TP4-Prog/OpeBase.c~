float addition (float x, float y)
{ 	
    float resultat;
    resultat=x+y;
    
    return resultat;
}
float soustraction (float x, float y)
{ 	
    float resultat;
    resultat=x-y;
    
    return resultat;
}
float miltiplication (float x, float y)
{ 	
    float resultat;
    resultat=x*y;
    
    return resultat;
}
float division (float x, float y)
{ 	
    float resultat;
    resultat=x/y;
    
    return resultat;
}